
&star_job

   ! this uses one of the prebuilt zams models from mesa/data/star_data
    change_net=.true.
    new_net_name="o18_ne22_inert_ni56.net"
    show_net_species_info=.true.
    show_net_reactions_info=.true.
    set_initial_dt = .true.
    years_for_initial_dt = 0.01
    load_saved_model=.true.
    load_model_filename="WD_064.mod"
    set_initial_model_number=.true.
    initial_model_number = 0
    set_initial_age=.true.
    initial_age=0
    
    show_log_description_at_start = .false.

    show_net_species_info = .true.

    set_initial_cumulative_energy_error = .true.
    new_cumulative_energy_error = 0d0

    !new_rotation_flag = .false.
    !change_rotation_flag = .false.
    !change_initial_rotation_flag = .true.
    !set_initial_surface_rotation_v = .true.
    !new_surface_rotation_v = 300d0

    ! Relaxes composition to that of a star with an initial mass
    ! of 10 Msun after TAMS, but before reaching the giant branch
    relax_initial_composition = .true.
    relax_composition_filename = "composition.dat"
    timescale_for_relax_composition = 1d-2 

    !relax_initial_angular_momentum = .false.
    !relax_angular_momentum_filename = "angular_momentum.dat"
    timescale_for_relax_angular_momentum = 1d-10
    max_dt_for_relax_angular_momentum = 1d-9
    num_timescales_for_relax_angular_momentum = 100

    relax_initial_entropy = .true.
    relax_entropy_filename = "entropy.dat"
    timescale_for_relax_entropy = 1d-10
    max_dt_for_relax_entropy = 1d-9
    num_timescales_for_relax_entropy = 100
    get_entropy_for_relax_from_eos= "eosDT" 
    save_model_when_terminate = .true.
    save_model_filename = 'relaxed_064.mod'
    pgstar_flag= .true.
    disable_pgstar_during_relax_flag= .false.
    pause_before_terminate = .true.
/ ! end of star_job namelist

&kap
  zbase=0.02  

/ ! end of kap namelist

&eos

/ ! end of eos namelist

&controls
    x_integer_ctrl(1) = 2

    ! we do just one step in this inlist, turning off gold for this single step helps
    use_gold_tolerances = .false.
    use_gold2_tolerances = .false.
    max_resid_jump_limit = 1d99

    !max_timestep = 1d2
    !max_model_number = 10
    max_years_for_timestep=0.1d0
    max_age=1
    !read_extra_controls_inlist(1) = .true.
    !extra_controls_inlist_name(1)= 'inlist_common'
    mesh_delta_coeff=1.0
    log_directory = 'LOGS_evolved_1year'

    scale_max_correction = 0.1d0

    varcontrol_target = 1d-3

  ! limit max_model_number as part of test_suite

    use_ledoux_criterion = .true.
    mixing_length_alpha = 1.5
    thermohaline_coeff = 1d2

  ! rotational mixing coeffs
    am_nu_ST_factor = 0.0
    D_visc_factor = 0.0
    am_nu_SH_factor = 0.0
    D_ST_factor = 0.0
    D_SH_factor = 0.0
    D_GSF_factor = 1.0
    D_ES_factor = 1.0
    D_SSI_factor = 1.0
    D_DSI_factor = 1.0
    am_D_mix_factor = 0.0333d0
    am_gradmu_factor = 0.1d0
    num_cells_for_smooth_gradL_composition_term = 3

    !hot_wind_scheme = 'Dutch'
    !cool_wind_RGB_scheme = 'Dutch'
    !cool_wind_AGB_scheme = 'Dutch'
    !Dutch_wind_lowT_scheme = 'de Jager'
    !Dutch_scaling_factor = 1.0
    max_abar_for_burning=-1

  ! controls for implicit wind
    surf_avg_tau_min = 0
    surf_avg_tau = 10
    max_mdot_redo_cnt = 200
    min_years_dt_for_redo_mdot = 0
    surf_omega_div_omega_crit_limit = 0.99d0
    surf_omega_div_omega_crit_tol = 0.06d0
    rotational_mdot_boost_fac = 1d10
    rotational_mdot_kh_fac = 1d10
    mdot_revise_factor = 1.2
    implicit_mdot_boost = 0.1
 

/ ! end of controls namelist


